# php -- END cPanel-generated handler, do not edit
RewriteCond %{HTTPS} off
RewriteCond %{HTTP:X-Forwarded-SSL} !on
RewriteCond %{HTTP_HOST} ^imdinventory\.com$ [OR]
RewriteCond %{HTTP_HOST} ^www\.imdinventory\.com$
RewriteRule ^/?$ "https\:\/\/www\.imdinventory\.com\/" [R=301,L]

